{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Console",
            "type": "mono",
            "request": "launch",
            "program": "${workspaceRoot}/EnerBank.Console/bin/Debug/netcoreapp1.0/EnerBank.Console.dll",
            "args": [
                "${workspaceRoot}/EnerBank.UI/Resources/accrediti.txt",
                "${workspaceRoot}/EnerBank.UI/Resources/estrazioni.txt"
                ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": "",
            "runtimeExecutable": null,
            "env": {},
            "externalConsole": true

        },
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/EnerBank.Ui.Web/bin/Debug/netcoreapp1.1/EnerBank.Ui.Web.dll",
            "args": [],
            "cwd": "${workspaceRoot}/EnerBank.Ui.Web/",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceRoot}/EnerBank.Ui.Web/Views"
            }
        }
    ]
}